import java.util.ArrayDeque;
import java.util.Scanner;

public class SimpleCalculator {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String input = sc.nextLine();

        ArrayDeque<String> result = new ArrayDeque<>();

        int num;
        int nextNum;
        int sum;
        int diff ;
        int charCount = 0;

        for (int i = 0; i < input.length(); i++) {

            char symbol = input.charAt(i);

            if (Character.isDigit(symbol) || symbol == '+' || symbol == '-'){
                String toAdd = Character.toString(symbol);
                result.push(toAdd);
                charCount++;

                if (charCount == 3){
                    if (result.contains("+")){

                         num = Integer.parseInt(result.getLast());
                         nextNum = Integer.parseInt(result.getFirst());
                         sum = num + nextNum;
                         result.clear();
                         String addSum = String.valueOf(sum);
                         result.push(addSum);
                         charCount = 1;


                    }else if (result.contains("-")){
                        num = Integer.parseInt(result.getFirst());
                        nextNum = Integer.parseInt(result.getLast());
                        diff = num - nextNum;
                        result.clear();
                        String addDiff = String.valueOf(diff);
                        result.push(addDiff);
                    }
                }
            }
        }

        for (String s : result) {
            System.out.print(s);
        }



    }
}
